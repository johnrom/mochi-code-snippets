    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "remark": "^14.0.1",
    "remark-code-import": "^0.4.0",
    "rollup": "^2.58.0",
    "typescript": "^4.4.4",
    "unified": "^10.1.0"
